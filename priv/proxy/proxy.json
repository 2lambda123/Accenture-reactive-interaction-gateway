[
  {
    "id": "identity-service",
    "name": "identity-service",
    "auth_type": "jwt",
    "auth": {
      "use_header": true,
      "header_name": "Authorization",
      "use_query": false,
      "query_name": ""
    },
    "versioned": false,
    "version_data": {
      "default": {
        "endpoints": [
          {
            "id": "post-is-auth-register",
            "path": "/is/auth/register",
            "method": "POST",
            "not_secured": true
          },
          {
            "id": "post-is-auth",
            "path": "/is/auth",
            "method": "POST",
            "not_secured": true
          },
          {
            "id": "get-is-user-info",
            "path": "/is/user-info",
            "method": "GET",
            "not_secured": false
          },
          {
            "id": "post-is-user-info",
            "path": "/is/user-info",
            "method": "POST",
            "not_secured": false
          },
          {
            "id": "get-is-users-id",
            "path": "/is/users/{id}",
            "method": "GET",
            "not_secured": false
          },
          {
            "id": "put-is-users-id",
            "path": "/is/users/{id}",
            "method": "PUT",
            "not_secured": false
          }
        ]
      }
    },
    "proxy": {
      "use_env": true,
      "target_url": "IS_HOST",
      "port": 7070
    }
  }
]