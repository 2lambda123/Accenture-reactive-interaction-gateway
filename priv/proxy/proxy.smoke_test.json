[
  {
    "id": "smoke-service",
    "name": "smoke-service",
    "versioned": false,
    "version_data": {
      "default": {
        "endpoints": [
          {
            "id": "get",
            "path_regex": "/api",
            "method": "GET"
          },
          {
            "id": "post",
            "path_regex": "/api",
            "method": "POST"
          },
          {
            "id": "put",
            "path_regex": "/api",
            "method": "PUT"
          },
          {
            "id": "patch",
            "path_regex": "/api",
            "method": "PATCH"
          },
          {
            "id": "delete",
            "path_regex": "/api",
            "method": "DELETE"
          },
          {
            "id": "head",
            "path_regex": "/api",
            "method": "HEAD"
          },
          {
            "id": "options",
            "path_regex": "/api",
            "method": "OPTIONS"
          },
          {
            "id": "post-kafka-produce",
            "path_regex": "/kafka/produce",
            "method": "POST"
          }
        ]
      }
    },
    "proxy": {
      "use_env": true,
      "target_url": "API_HOST",
      "port": 8000
    }
  }
]
